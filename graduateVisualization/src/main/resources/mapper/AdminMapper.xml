<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="edu.gyj.graduate.mapper.AdminUserMapper">
    <resultMap id="AdminResultMap" type="edu.gyj.graduate.entity.AdminUser">
        <result column="name" property="name"/>
        <result column="password" property="password"/>
    </resultMap>
    <select id="findByName" resultMap="AdminResultMap">
        select * from admin_user where name = #{name}
    </select>

</mapper>